<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.history.HistoryContainerFragment">

    <fragment
        android:id="@+id/fragment_history"
        android:name="id.dev.oxy.ui.history.HistoryFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.35"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/fragment_history_detail"
        android:name="id.dev.oxy.ui.history.HistoryDetailFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintStart_toEndOf="@+id/fragment_history"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.65"
        tools:layout="@layout/fragment_history_detail" />

</androidx.constraintlayout.widget.ConstraintLayout>